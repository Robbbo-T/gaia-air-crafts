{
  "name": "gaia-air-crafts",
  "version": "1.1.0",
  "private": true,
  "workspaces": [
    "GP-AM",
    "GP-FD",
    "GP-AS",
    "GP-COM",
    "GP-RAME",
    "GP-RAME/packages/*",
    "GP-SUPL",
    "GP-PM",
    "twins/*",
    "apps/doc-viewer"
  ],
  "scripts": {
    "start": "yarn workspace doc-viewer dev",
    "build": "lerna run build --stream",
    "test": "lerna run test --stream",
    "generate-nav": "node scripts/export-nav.js",
    "dashboard": "yarn workspace @gaia-air/rbt-learning-dashboard dev",
    "build:dashboard": "yarn workspace @gaia-air/rbt-learning-dashboard build"
  },
  "dependencies": {
    "@gaia-air/rbt-learning-dashboard": "workspace:*"
  },
  "devDependencies": {
    "lerna": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
